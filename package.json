    "ember-router-service-refresh-polyfill": "^1.0.0",
    "ember-simple-auth": "^4.2.1",
    "ember-source": "~4.12.0",
    "ember-template-lint": "^5.7.2",
    "ember-truth-helpers": "^3.0.0",
    "eslint": "^8.37.0",
